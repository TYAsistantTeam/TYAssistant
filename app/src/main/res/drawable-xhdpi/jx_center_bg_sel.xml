<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape>
            <solid android:color="@color/jx_blue" />
            <size android:height="30.0dip" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/jx_blue" />
            <size android:height="30.0dip" />
        </shape>
    </item>
    <item android:state_checked="false">
        <layer-list>
            <item android:left="-1.0dip">
                <shape>
                    <solid android:color="@color/whirte" />
                    <stroke android:width="1.0dip" android:color="@color/jx_blue" />
                    <size android:height="30.0dip" />
                </shape>
            </item>
        </layer-list>
    </item>
</selector>